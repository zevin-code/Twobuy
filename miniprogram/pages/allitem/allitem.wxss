
page{  
  background: #f0f0f0;  
}  

.search-bar-item{
  height: 64rpx;
  border-radius: 32rpx;
  background-color: #FAFAFA;
  font-size: 24rpx;
  color: #BCC3CB;
}
 
.container {  
  position: relative;  
  width: 100%;  
  height: 100%;  
  margin-top: 16rpx;
}
 
.nav_left{  
  display: inline-block;  
  width: 30%;  
  height: 100%;  
  background: #fff;  
  text-align: center; 
  position: fixed; top: 16rpx; left: 0; 
  border-radius: 0 20rpx 20rpx 0;
  padding: 36rpx 0;
}  

.nav_left .nav_left_items{  
  height: 106rpx;  
  line-height: 40px;   
  font-size: 28rpx; 
}  

.nav_left .nav_left_items.active{  
  font-size: 32rpx;
  color: #F25551; 
}  
 
.nav_right{  
  position: absolute;  
  top: 0;  
  right: 0;  
  flex: 1;  
  width: 68%;  
  height: 1000px;  
  padding: 50rpx 0; 
  padding-left: 56rpx; 
  box-sizing: border-box;  
  background: #fff;  
  border-radius: 20rpx 0 0 20rpx;
  display: flex;
  justify-content: space-around;
}  

.nav_right .nav_right_items{  
  width: 166rpx;  
  height: 250rpx;  
  text-align: center; 
  margin-bottom: 90rpx;
  margin-right: 56rpx;
  display: inline-block;
}  

.nav_right .nav_right_items image{  
  width: 166rpx;  
  height: 198rpx; 
  background-color: #f5f5f5; 
  border-radius: 20rpx; 
}  

.nav_right .nav_right_items text{    
  display: block;  
  margin-top: 10rpx;  
  font-size: 28rpx;  
  color: #303030;
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;
} 

.nodata_text
{
  color: black;
  font-size: 14px;  
  text-align: center;  
} 